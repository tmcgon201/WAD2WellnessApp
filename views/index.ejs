    <!-- contact form -->
    <section id="contact" class="container px-4 py-5" style="background-color:#F9F9F9">
        <!-- heading -->
        <h3 class='text-center pb-1 font-weight-light'><strong>CONTACT</strong></h3>
        <h6 class='text-center font-weight-normal pt-3'>If you have any further queries, please contact us using the form below.</h6>
        <h6 class='text-center font-weight-normal pb-3'>We aim to respond to every enquiry within 3 business days.</h6>
        <div class="row mx-auto w-50 bg-white rounded shadow-sm pb-4">
            <div class="col mx-auto pb-3">
                <!-- form -->
                <form id="contact-form" name="contact-form" action="/sendEmail" method="POST">
                    <div class="row mb-3">
                      <!-- email subject -->
                      <div class="col-md-12">
                          <div class="md-form">
                                <input type="text" id="email" name="email" class="form-control" placeholder="Email" required/>
                          </div>
                      </div>
                  </div>
                    <div class="row mb-3">
                        <!-- message subject -->
                        <div class="col-md-12">
                            <div class="md-form">
                                <input type="text" id="subject" name="subject" class="form-control" placeholder="Subject" required/>
                            </div>
                        </div>
                    </div>
                    <div class="row mb-3">
                        <!-- message -->
                        <div class="col-md-12">
                            <div class="md-form">
                                <textarea type="text" id="message" name="message" rows="2"
                                    class="form-control md-textarea" placeholder="Your message" required></textarea>
                            </div>
                        </div>
                    </div>
                    <!-- send button -->
                    <div class="text-center mx-auto">
                        <button class="btn btn-block shadow-sm" style='background-color:#98fbca'
                            type="submit">Send</button>
                    </div>
                </form>        
            </div>
        </div>
    </section>
