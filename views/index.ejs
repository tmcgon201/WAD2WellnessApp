<style>
    .image-container {
      position: relative;
      display: inline-block;
      transition: transform 0.5s;
      margin-left: -300px;
    }

    .image-container:hover img {
      opacity: 0.5;
    }

    .image-container:hover .text-overlay .fact {
      opacity: 1;
    }

    .image-container:hover .text-overlay {
      opacity: 1;
      transform: translate(-50%, -60%);
    }

    .text-overlay {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      text-align: center;
      color: black;
      opacity: 0;
      transition: opacity 0.5s, transform 0.5s;
    }

    .text-overlay p {
      font-size: 16px;
      font-weight: bold;
    }

    .below-image {
      font-size: 24px;
      font-weight: bold;
      margin-top: 1%;
      text-align: center;
    }

    .factinfo {
      color: black;
      text-shadow: white;
      text-decoration-color: black;
      font-size: 1px;
    }

    #menu {
      margin-left: 0px;
    }

    .image-container,
    .image-container-two,
    .image-container-three {
      position: relative;
      display: inline-block;
      transition: transform 0.5s;
      vertical-align: top;
      margin-left: -300px;
  }
  .image-container-two {
      margin-left: 150px;
  }
  #about{
      position: relative;
  }

  .image-container-three {
      left: 450px;
  }
</style>
